{
  "name": "convert-lara-answers",
  "version": "0.0.1",
  "description": "",
  "main": "src/convert-answers.ts",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/",
      "/dist/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "test": "jest",
    "start": "npm run build && node --max-old-space-size=8000 dist/convert-lara-answers/src/convert-answers.js"
  },
  "author": "Piotr Janik",
  "license": "MIT",
  "dependencies": {
    "@firecode/admin": "0.12.1",
    "@google-cloud/firestore": "^5.0.2",
    "@qualdesk/firestore-big-batch": "^0.0.2",
    "deep-equal": "^2.0.5"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
