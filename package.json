{
  "name": "question-interactives",
  "version": "1.6.0",
  "description": "Concord Consortium question interactives",
  "main": "index.js",
  "jest": {
    "testURL": "https://question-interactives.unexisting.url.com",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --hot",
    "start:secure": "webpack-dev-server --https --hot --cert ~/.localhost-ssl/localhost.pem --key ~/.localhost-ssl/localhost.key",
    "start:secure:no-certs": "webpack-dev-server --https --hot",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "webpack --mode production --devtool source-map",
    "clean": "rimraf dist",
    "lara-api:install": "npm install",
    "lara-api:link": "npm link @concord-consortium/lara-interactive-api",
    "lara-api:unlink:only": "npm unlink --no-save @concord-consortium/lara-interactive-api",
    "lara-api:unlink": "npm-run-all lara-api:unlink:only lara-api:install",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \".eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:full": "npm-run-all test test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/question-interactives.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/question-interactives/issues"
  },
  "homepage": "https://github.com/concord-consortium/question-interactives#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/chart.js": "^2.9.28",
    "@types/deep-equal": "^1.0.1",
    "@types/dompurify": "^2.0.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/object-hash": "^2.1.1",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-jsonschema-form": "^1.7.4",
    "@types/uuid": "^8.3.0",
    "@types/video.js": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^9.8.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "canvas": "2.6.1",
    "css-loader": "^4.2.2",
    "cypress": "^9.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^8.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.0.0",
    "generate-json-from-js-webpack-plugin": "^0.1.1",
