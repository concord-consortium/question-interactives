{
  "name": "question-interactives",
  "version": "0.0.1",
  "description": "Concord Consortium question interactives",
  "main": "index.js",
  "jest": {
    "testURL": "https://question-interactives.unexisting.url.com",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base dist/",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "webpack --mode production --devtool false",
    "clean": "rimraf dist",
    "lara-api:install": "npm install",
    "lara-api:link": "npm link @concord-consortium/lara-interactive-api",
    "lara-api:unlink:only": "npm unlink --no-save @concord-consortium/lara-interactive-api",
    "lara-api:unlink": "npm-run-all lara-api:unlink:only lara-api:install",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:build": "eslint -c \".eslintrc.build.js\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watchAll",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:full": "npm-run-all test test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/question-interactives.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/question-interactives/issues"
  },
  "homepage": "https://github.com/concord-consortium/question-interactives#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/deep-equal": "^1.0.1",
    "@types/dompurify": "^2.0.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-jsonschema-form": "^1.7.4",
    "@types/uuid": "^8.3.0",
    "@types/video.js": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "autoprefixer": "^9.8.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "cypress": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint": "^7.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.1",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@concord-consortium/lara-interactive-api": "0.5.0-pre.4",
    "@concord-consortium/slate-editor": "^0.6.0",
    "deep-equal": "^2.0.3",
    "dompurify": "^2.0.14",
    "drawing-tool": "^2.0.1",
    "firebase": "^7.19.0",
    "html-react-parser": "^0.13.0",
    "iframe-phone": "^1.3.1",
    "intersection-observer": "^0.11.0",
    "jquery": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jsonschema-form": "^1.8.1",
    "resize-observer-polyfill": "^1.5.1",
    "shutterbug": "^1.3.0",
    "striptags": "^3.1.1",
    "uuid": "^8.3.0",
    "video.js": "^7.8.4"
  }
}
