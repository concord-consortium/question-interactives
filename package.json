    "acorn": "^8.7.0",
    "chart.js": "^2.5.0",
    "chartjs-plugin-datalabels": "^1.0.0",
    "classnames": "^2.3.1",
    "deep-equal": "^2.0.3",
    "deepmerge": "^4.2.2",
    "dompurify": "^2.3.6",
    "drawing-tool": "^2.1.2",
    "eslint-webpack-plugin": "^3.1.1",
    "firebase": "^9.6.7",
    "html-react-parser": "^0.13.0",
    "iframe-phone": "^1.3.1",
    "intersection-observer": "^0.11.0",
    "jquery": "^3.5.1",
    "object-hash": "^2.2.0",
    "query-string": "^7.0.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.11.1",
    "react-dnd": "^11.1.3",
